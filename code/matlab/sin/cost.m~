function costval = cost(x,y,network)

    xn=length(x);
    costvec = zeros(xn,1); 
    
    for i = 1:xn
        
        input=x(:,i);
        output=y(:,i);
        
        a(1)={input};
        for j=2:L
            a(i)={activate(a{i-1},network{1,i},network{2,i})};
        end
        
        costvec(i)=norm(output-a{L},2);
    end
    
    costval = norm(costvec,2)^2;